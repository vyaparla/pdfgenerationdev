<div id="main-container" class="container-fluid">
  <div class="row">
    <div class="col-xs-12">
      <ul class="nav nav-tabs" role="tablist">
        <li><%= link_to 'Damper Inspection',  pdfjobs_path %></li>
        <li class="active"><%= link_to 'Damper Repair',  damper_repair_pdfjobs_path %></li>
        <li><%= link_to 'FireDoor Inspection',  firedoor_inspection_pdfjobs_path %></li>
        <li><%= link_to 'Firestop Survey',  firestop_survey_pdfjobs_path %></li>
        <li><%= link_to 'Firestop Installation',  firestop_installation_pdfjobs_path %></li>
      </ul>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-12">
      <div class="col-xs-6">
        <h1>Listing of Damper Repair(<%= @damper_repair_count %>)</h1>
      </div>
      <div class="col-xs-6">
        <%= will_paginate  @damper_repair %>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-12">
      <div class="table-responsview">
        <table class="table table-bordered border-striped" cellspacing="0" width="100%" id ="example">
          <thead>
            <tr>
              <!-- <th>Group Name(u_facility_name)</th>
              <th>Facility Name(u_group_name)</th> -->
              <th>Job ID(u_job_id)</th>
              <!-- <th>Job Start Date(u_job_start_date)</th>
              <th>Job End Date(u_job_end_date)</th> 
              <th>Facility SysId(u_facility_sys_id)</th> -->
              <th>Service ID(u_service_id)</th>
              <!-- <th>Asset ID(u_asset_id)</th>
              <th>Building(u_building)</th> -->
              <th>Floor(u_floor)</th>
              <th>Type(u_type)</th>
              <th>Damper Name(u_damper_name)</th>
              <th>Asset Name(u_tag)</th>
              <th>Status(u_dr_passed_post_repair)</th>
              <th>Repair Action Performed(u_repair_action_performed)</th>
              <th>Repair Date(u_inspected_on)</th>
              <th>Active(u_active)</th>
              <th>Delete(u_delete)</th>
              <!-- <th>Location(u_location_desc)</th>
              <th>Damper Repaired Description(u_dr_description)</th>
              <th>u_dr_damper_model</th>
              <th>u_dr_installed_damper_type</th>
              <th>u_dr_installed_damper_height</th>
              <th>u_dr_installed_damper_width</th>
              <th>u_dr_installed_actuator_model</th>
              <th>u_dr_installed_actuator_type</th>
              <th>u_dr_actuator_voltage</th>
              <th>Access Door(u_access_size)</th> -->
              <th colspan="3"></th>
            </tr>
          </thead>
          <tbody>
            <% @damper_repair.each do |dr| %>
              <tr>
                <td><%= dr.u_job_id %></td>                
                <td><%= dr.u_service_id %></td>
                <td><%= dr.u_floor.to_i %></td>
                <td><%= dr.u_type %></td>
                <td><%= dr.u_damper_name %></td>
                <td><%= dr.u_tag %></td>
                <td><%= dr.u_dr_passed_post_repair %></td>
                <td><%= dr.u_repair_action_performed %></td>
                <td><%= dr.u_inspected_on %></td>
                <td><%= dr.u_active %></td>
                <td><%= dr.u_delete %></td>
                <% if current_user.role == "Admin" %>
                  <td><%= link_to 'Destroy', pdfjob_path(dr), method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger btn-xs' %></td>
                <% end %>
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>