<%= @pdfjobs_count %><br/>

<p style="color:red;"><%= flash[:success] %></p>
<p style="margin: 0 0 0 943px;"><%= link_to "Delete All",  clear_index_list_view_pdfjobs_path %></p>

<table style="border: 1px solid #cacaca;" width="80%" align="top" cellpadding="0" cellspacing="0">
  <tbody>
    <tr>
      <th>Job ID</th>
      <th>Service ID</th>
      <th>Location</th>
      <th>Bulding</th>
      <th>Floor</th>
      <th>Damper Type</th>
      <th>Damper Name</th>
      <th>Result</th>
      <th>Job Start Date</th>
      <th>Job End Data</th>
      <th>Inspected ON</th>
      <th>Action</th>
    </tr>
    <% @pdfjobs.each do |job| %>
      <tr>
        <td><%= job.u_job_id %></td>
        <td><%= job.u_service_id %></td>
        <td><%= job.u_location_desc %></td>
        <td><%= job.u_building %></td>
        <td><%= job.u_floor %></td>
        <td><%= job.u_type %></td>
        <td><%= job.u_damper_name %></td>
        <td><%= job.u_status %></td>
        <td><%= job.u_job_start_date %></td>
        <td><%= job.u_job_end_date %></td>
        <td><%= job.u_inspected_on %></td>
       
        <!--<td><%#= job.u_pdf_number %></td>-->
        <!--<td><%#= image_tag("data:image/jpeg;base64,#{job.image1}", size: "100") %></td>-->
        <!--<td><%#= image_tag("data:image/jpeg;base64,#{job.u_close_image_base64code}", size: "100") %></td> -->
        <td><%= link_to 'Delete', pdfjob_path(job),  method: :delete, 
        data: { confirm: 'Are you sure?' } %></td>
        <%# if job.has_full_report? %>
          <!--<td><%#= link_to "Download Report", download_full_pdf_report_pdfjobs_path(:id => job.id), target: '_self' %></td>-->
        <%# else %>
          <!--<td><%#= link_to "Generate Report", generate_full_pdf_report_pdfjobs_path(:id => job.id)%></td>-->
        <%# end %>
      </tr>
    <% end %>
  </tbody>
</table>
