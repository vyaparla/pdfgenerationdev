<div id="main-container" class="container-fluid">
  <div class="row">
    <div class="col-xs-12">
      <ul class="nav nav-tabs" role="tablist">
        <li><%= link_to "MPC Damper Inspection", project_completions_path %></li>
        <li><%= link_to "MPC Damper Repair", mpc_damperrepair_project_completions_path %></li>
        <li><%= link_to "MPC FireDoor Inspection", mpc_firedoorinspection_project_completions_path %></li>
        <li><%= link_to "MPC Firestop Survey", mpc_firestopsurvey_project_completions_path %></li>
        <li class="active"><%= link_to "MPC Firestop Installation", mpc_firestopinstallation_project_completions_path %></li>
      </ul>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-12">
      <div class="col-xs-6">
        <h1>Listing of Firestop Survey(<%= @mpc_fsi_count %>)</h1>
      </div>
      <div class="col-xs-6">
        <%= will_paginate  @mpc_fsi %>
      </div>
    </div>
  </div>
  <div class="row">
    <div class="col-xs-12">
      <div class="table-responsview">
        <table class="table table-bordered border-striped" cellspacing="0" width="100%" id ="example">
          <thead>
            <tr>
              <th>Job ID(m_job_id)</th>
              <th>Service SysId(m_service_sysid)</th>
              <th>Date(m_date)</th>
              <th>Facility(m_facility)</th>
              <th>Building(m_building)</th>
              <th>Project Start Date(m_project_start_date)</th>
              <th>Primary Contact Name(m_primary_contact_name)</th>
              <th>Phone#(m_phone)</th>
              <th>Printed Report(m_printed_report)</th>
              <th>Autocad(m_autocad)</th>
              <th>Total Firestop Installation Surveyed(m_total_no_of_fsi_surveyed)</th>
              <th>Total Firestop Installation Fixed On Site(m_total_no_of_fsi_fixedonsite)</th>              
              <th>Containment Tent Used(m_containment_tent_used)</th>
              <th>Base Bid Count(m_base_bid_count)</th>
              <th>Total Authorized Damper Bid(m_new_total_authorized_damper_bid)</th>
              <th>Technician Name(m_technician_name)</th>
              <th>Blueprints Facility(m_blueprints_facility)</th>
              <th>Replacement Checklist(m_replacement_checklist)</th>
              <th>Facility Items(m_facility_items)</th>
              <th>Emailed Reports(m_emailed_reports)</th>
              <th>Daily Basis(m_daily_basis)</th>
              <th colspan="3"></th>
            </tr>
          </thead>
          <tbody>
            <% @mpc_fsi.each do |fsi| %>
              <tr>
                <td><%= fsi.m_job_id %></td>
                <td><%= fsi.m_service_sysid %></td>
                <td><%= fsi.m_date.localtime.strftime(I18n.t('time.formats.mdY')) %></td>
                <td><%= fsi.m_facility %></td>
                <td><%= fsi.m_building %></td>
                <td><%= fsi.m_project_start_date.localtime.strftime(I18n.t('time.formats.mdY')) %></td>
                <td><%= fsi.m_primary_contact_name %></td>
                <td><%= fsi.m_phone %></td>
                <td><%= fsi.m_printed_report %></td>
                <td><%= fsi.m_autocad %></td>
                <td><%= fsi.m_total_no_of_fsi_surveyed %></td>
                <td><%= fsi.m_total_no_of_fsi_fixedonsite %></td>                
                <td><%= fsi.m_containment_tent_used %></td>
                <td><%= fsi.m_base_bid_count %></td>
                <td><%= fsi.m_new_total_authorized_damper_bid %></td>
                <td><%= fsi.m_technician_name %></td>
                <td><%= fsi.m_blueprints_facility %></td>
                <td><%= fsi.m_replacement_checklist %></td>
                <td><%= fsi.m_facility_items %></td>
                <td><%= fsi.m_emailed_reports %></td>
                <td><%= fsi.m_daily_basis %></td>
                <% if current_user.role == "Admin" %>
                  <td><%= link_to "Destroy", project_completion_path(fsi), method: :delete, data: { confirm: 'Are you sure?' }, class: 'btn btn-danger btn-xs' %></td>
                <% end %> 
              </tr>
            <% end %>
          </tbody>
        </table>
      </div>
    </div>
  </div>
</div>